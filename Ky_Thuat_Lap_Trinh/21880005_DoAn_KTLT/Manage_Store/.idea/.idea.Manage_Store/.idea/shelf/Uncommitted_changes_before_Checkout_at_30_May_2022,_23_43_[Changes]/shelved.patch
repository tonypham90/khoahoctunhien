<+>@page\n@using Microsoft.AspNetCore.Http.Features\n@using Manage_Store.Entity\n@model IndexModel\n@{\n    ViewData[\"Title\"] = \"Home page\";\n}\n<header>\n  <h1 class=\"h1\">Kho Hang</h1>\n</header>\n<body>\n<h6>Chuc Nang</h6><br/>\n<section class=\"btn-group-lg\">\n        <button class=\"btn btn-outline-primary\" onclick=\"window.location.href='/ItemCreate'\">Tao San Pham Moi</button>\n        <button class=\"btn btn-outline-primary\" onclick=\"window.location.href='/LabelCreate'\">Cap Nhat Loai Hang</button>\n    </section>\n\n<section>\n    <fieldset class=\"border-primary\">\n        <legend title=\"Tim kiem\">\n            <form method=\"post\">\n                <label for=\"find\" class=\"form-label\">Chuc nang tim kiem</label>\n                <select name=\"choicefunc\">Tim theo:\n                    <option value=\"0\">Tat ca</option>\n                    <option value=\"1\">Ma San Pham</option>\n                    <option value=\"2\">Ten San pham</option>\n                    <option value=\"3\">Ten Cong Ty</option>\n                    <option value=\"4\">Loai Hang</option>\n                </select>\n                <label for=\"keyword\">Keyword</label>\n                <input type=\"text\" name=\"keyword\" value=\"@Model.Keyword\"/>\n                <input type=\"submit\" value=\"Tim kiem\"/>\n            </form>\n        </legend>\n    </fieldset>\n</section>\n<section1>\n    <table title=\"San Pham trong kho\" class=\"table\">\n        <h1 class=\"align-self-xxl-center\"> Danh Sach San Pham</h1>\n        <tr class=\"text-center table-primary\">\n            <th>Ma san pham</th>\n            <th>Ten</th>\n            <th>Cty San Xuat</th>\n            <th>So luong</th>\n            <th>Ngay san xuat</th>\n            <th>Han su dung</th>\n            <th>Gia</th>\n            <th>Loai</th>\n            <th>Chuc Nang</th>\n        </tr>\n        \n            @{\n                foreach (StrucItem Item in @Model.ItemsShow)\n                {\n                    <tr>\n                        <td>@Item.Id</td>\n                        <td>@Item.Name</td>\n                        <td>@Item.Manufacture</td>\n                        <td>@Item.Qty.ToString(\"N0\")</td>\n                        <td>@Item.Mfg</td>\n                        <td>@Item.Exp</td>\n                        <td>@Item.Price.ToString(\"N0\")</td>\n                        <td>@Item.Label</td>\n                        <td>\n                            <button class=\"btn btn-outline-secondary\" onclick=\"window.location.href='/ImportNew?ItemId=@Item.Id'\">Nhap Hang</button>\n                            <button class=\"btn btn-outline-secondary\" onclick=\"window.location.href='/itemupdate?id=@Item.Id'\">Cap Nhat</button>\n                            <button class=\"btn btn-outline-secondary\" onclick=\"window.location.href='/itemremove?id=@Item.Id'\">Xoa</button>\n                        </td>\n                    </tr>\n                }\n            }\n    </table>\n</section1>\n</body>
